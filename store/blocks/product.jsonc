{
  "store.product": {
    "children": [
      "flex-layout.row#product"
    ]
  },
  "flex-layout.row#product": {
    "children": [
      "flex-layout.row#productimage",
      "flex-layout.row#productDetails"
    ],
    "props": {
      "blockClass": "viewProduto"
    }
  },
  "flex-layout.row#productimage": {
    "children": [
      "product-images"
    ]
  },
  "flex-layout.row#productDetails": {
    "children": [
      "product-name",
      "product-price"
    ],
    "props":{
      "blockClass": "productDetails"
    }
  },
  "product-name": {
    "props": {
      "showSku": true,
      "showBrandName": true
    }
  },
  "product-price": {
    "props": {
      "showListPrice": true,
      "showLabels": false
    }
  },
  "product-images": {
    "props": {
      "displayThumbnailsArrows": true
    }
  }
}